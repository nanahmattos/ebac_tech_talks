over;\r\n    position: relative;\r\n\r\n    .container {\r\n        position: relative;\r\n    }\r\n\r\n    &::before {\r\n        position: absolute; //para funcionar é necessario adicionar o position relative no container pai.\r\n        top: 0;\r\n        left: 0;\r\n        width: 100%;\r\n        height: 100%;\r\n        background-color: variables.$secondaryTextColor;\r\n        content: '';\r\n        opacity: 0.7;\r\n    }\r\n\r\n    &__text{\r\n        margin-top: 64px;\r\n        margin-bottom: 64px;\r\n        font-size: 40px;\r\n        line-height: 56px; // espaçamento entre paragrafos\r\n        font-weight: 800;\r\n        text-align: center;\r\n    }\r\n\r\n    &__logo {\r\n        margin: 0 auto;\r\n        height: 80px;\r\n        display: block;\r\n    }\r\n\r\n    @media (max-width: 640px) {\r\n        padding: 40px 0;\r\n\r\n        &__logo {\r\n            height: 40px;\r\n        }\r\n\r\n        &__text {\r\n            font-size: 22px;\r\n            line-height: 24px;\r\n            margin: 24px 0;\r\n        }\r\n    }\r\n}","styles\\_variables.scss":"$textColor: #fff;\r\n$secondaryTextColor: #000;\r\n\r\n$blue1: #1B6CA7;\r\n$blue2: #442BDB;\r\n$pink: #89223B;\r\n$yellow: #9C791D;\r\n$purple: #4F2AC3;","styles\\components\\_infos_bar.scss":"@use '../variables';\r\n\r\n.infos-bar {\r\n    padding: 16px 24px;\r\n    background-color: variables.$textColor;\r\n    display: flex;\r\n    justify-content: space-between;\r\n\r\n    &__infos {\r\n        display: flex;\r\n        gap: 40px; //espaçamento laterais\r\n\r\n        &__item {\r\n            font-size: 14px;\r\n            color: variables.$secondaryTextColor;\r\n\r\n            b {\r\n                color: variables.$secondaryTextColor;\r\n                font-size: 18px;\r\n                display: block;\r\n            }\r\n        }\r\n    }\r\n    \r\n    @media (max-width: 640px) {  //resolve scroll na horizontal.\r\n        display: block;\r\n\r\n        &__infos {\r\n            display: block;\r\n\r\n            &__item {\r\n                margin-bottom: 24px;\r\n            }\r\n        }\r\n    }\r\n\r\n    @media (max-width: 1024px ){\r\n        &__infos {\r\n            gap: 16px;\r\n        }\r\n    }\r\n}","styles\\components\\_buttons.scss":"@use '../variables';\r\n\r\n.button {\r\n    padding: 12px;\r\n    font-size: 18px;\r\n    font-weight: bold;\r\n    text-decoration: none;\r\n    text-align: center;\r\n    white-space: nowrap; //quebrar ou nao a linha\r\n\r\n    &--primary {\r\n        color: variables.$textColor;\r\n        background-color: variables.$purple;\r\n    }\r\n\r\n    @media (max-width: 640px) {\r\n        display: block;\r\n        width: 100%;\r\n    }\r\n\r\n    @media (max-width: 1024px) {\r\n        display: flex;\r\n        align-items: center;\r\n        \r\n    }\r\n}","styles\\_event.scss":"@use 'variables';\r\n\r\n.event {\r\n    padding: 80px 0;\r\n    \r\n    &--frontend {\r\n        background-image: linear-gradient(variables.$blue1, variables.$blue2);\r\n    }\r\n    &--ui-ux {\r\n        background-image: linear-gradient(variables.$blue2, variables.$pink);\r\n    }\r\n    &--data-science {\r\n        background-image: linear-gradient(variables.$pink, variables.$yellow);\r\n    }\r\n    &--backend {\r\n        background-image: linear-gradient(variables.$yellow, variables.$pink);\r\n    }\r\n\r\n    &--image-left { //modificador de direção \r\n        .container {\r\n            flex-direction: row-reverse;\r\n        }\r\n    }\r\n\r\n    .container {\r\n        display: flex;\r\n        gap: 40px;\r\n    }\r\n\r\n    &__image {\r\n        max-width: 360px;\r\n    }\r\n\r\n    &__details {\r\n        \r\n        &__title {\r\n            font-size: 18px;\r\n            font-weight: bold;\r\n            margin-bottom: 16px;\r\n        }\r\n\r\n        &__description {\r\n            line-height: 22px;\r\n        }\r\n    }\r\n\r\n    @media (max-width: 640px) {\r\n        .container {\r\n            display: block;\r\n        }\r\n        &__image {\r\n            width: 100%;\r\n        }\r\n        &__details {\r\n            margin-right: 0;\r\n            margin-bottom: 16px;\r\n        }\r\n    }\r\n}","styles\\main.scss":"@use 'hero';\r\n@use 'components/infos_bar';\r\n@use 'components/buttons';\r\n@use 'event';\r\n@use 'variables';\r\n\r\n* {\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n    font-family: 'Roboto', sans-serif;\r\n    color: variables.$textColor;\r\n    list-style: none;\r\n}\r\n\r\n.container {\r\n    max-width: 960px;\r\n    width: 100%;\r\n    margin: 0 auto;\r\n\r\n    @media (max-width: 640px) {\r\n        max-width: 80%;\r\n    }\r\n\r\n    @media (max-width: 1024px){\r\n        max-width: 90%;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"938310424949dee14d56cb7bc6138f6a","cacheData":{"env":{}}}